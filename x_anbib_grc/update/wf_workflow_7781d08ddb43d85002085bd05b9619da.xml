        <value>workflow.scratchpad.children = [];&#13;
var entity = current.sys_id.toString();&#13;
var type = current.type.toString();&#13;
// Verifica se foi update e se sim, se teve alteração de tipo e layer&#13;
&#13;
// If type cheanged, disable controls&#13;
if (type &amp;&amp; workflow.variables.u_old_type &amp;&amp; type != workflow.variables.u_old_type) {&#13;
	// Insert control&#13;
	var grOldControl = new GlideRecord("x_anbib_grc_control");&#13;
	grOldControl.addQuery("entity", entity);&#13;
	grOldControl.addQuery("layer.entity_type", workflow.variables.u_old_type);&#13;
	grOldControl.state = "8";  // disabled - type changed&#13;
	grOldControl.active = false;&#13;
	grOldControl.updateMultiple();	&#13;
}&#13;
&#13;
updateEntityControls(entity, type);&#13;
&#13;
function updateEntityControls(entity, type) {&#13;
	var grEntityLayer = new GlideRecord("x_anbib_grc_m2m_entities_layer_controls");&#13;
	grEntityLayer.addQuery("entity", entity);&#13;
	grEntityLayer.query();&#13;
	while (grEntityLayer.next()) {&#13;
		var layer = grEntityLayer.layer_control.toString();&#13;
&#13;
		var grLayerControlTpl = new GlideRecord("x_anbib_grc_m2m_control_temp_layer_contro");&#13;
		grLayerControlTpl.addQuery("layer_control", layer);&#13;
		grLayerControlTpl.query();&#13;
		while (grLayerControlTpl.next()) {&#13;
			var template = grLayerControlTpl.control_template.toString();&#13;
&#13;
			// Insert control&#13;
			var grControl = new GlideRecord("x_anbib_grc_control");&#13;
			grControl.addQuery("entity", entity);&#13;
			grControl.addQuery("control", template);&#13;
			grControl.addQuery("layer", layer);&#13;
			grControl.addQuery("layer.entity_type", type);&#13;
			grControl.query();&#13;
&#13;
			if (!grControl.next()) {&#13;
				// Add control definition to array&#13;
// 				workflow.scratchpad.controls.push({&#13;
// 					u_entity: entity,&#13;
// 					u_control: template,&#13;
// 					u_layer: layer&#13;
// 				});&#13;
				grControl.initialize();&#13;
				grControl.entity = entity;&#13;
				grControl.control = template;&#13;
				grControl.layer = layer;&#13;
				grControl.state = "0";&#13;
				grControl.frequency = grLayerControlTpl.frequency;&#13;
				grControl.period = grLayerControlTpl.period;&#13;
				grControl.period_unit = grLayerControlTpl.period_unit;&#13;
				&#13;
				var control_sys_id = grControl.insert();  // inserted control sys_id&#13;
				&#13;
				var grControlInsert = new GlideRecord('x_anbib_grc_control');&#13;
				grControlInsert.get(control_sys_id);      // inserted control record&#13;
				&#13;
				var wf = new global.Workflow();&#13;
				var thisEntityFlows = wf.getRunningFlows(current); // Opened flows for entity&#13;
				var thisContext = "";&#13;
				while(thisEntityFlows.next()) {&#13;
					thisContext = thisEntityFlows.getUniqueValue();&#13;
				}&#13;
				// Parametros para subflow&#13;
				var inputs = {					&#13;
					u_fatherwkf: thisContext,&#13;
					u_record: current.sys_id.toString()&#13;
				};&#13;
				var wf_sys_id = wf.startFlow(gs.getProperty("x_anbib_grc.workflow_control"), grControlInsert, "insert", inputs);&#13;
				&#13;
				workflow.scratchpad.children.push({&#13;
					record: control_sys_id,&#13;
					childWKF: wf_sys_id,&#13;
					status: "running"&#13;
				});&#13;
			} else {&#13;
				if (!grControl.active) {&#13;
					grControl.active = true;&#13;
					grControl.state = "1";  // review&#13;
					grControl.update();&#13;
				}&#13;
			}&#13;
		}&#13;
	}&#13;
}</value>
