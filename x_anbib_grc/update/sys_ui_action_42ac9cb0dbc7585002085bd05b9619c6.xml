        <onclick/>
        <order>100</order>
        <script><![CDATA[var state = current.state.toString();
if (state == "2" && current.status == "1") {   // não precisa reiniciar o processo, a evidencia ainda não foi validada
	current.expected_state = '2';
	current.update();
	redirect();
} else if (state == "0" || state == "1") {
	current.expected_state = '2';
	current.state = '2';
	current.update();
	redirect();
} else {
	new global.Workflow().restartWorkflow(current);
	current.expected_state = '2';
	current.state = '2';
	current.update();
	redirect();
}

function redirect() {
	var isList = action.getGlideURI().get('sys_is_list');
	if (!isList) //Not from list form
		action.setRedirectURL(current);
}]]></script>
