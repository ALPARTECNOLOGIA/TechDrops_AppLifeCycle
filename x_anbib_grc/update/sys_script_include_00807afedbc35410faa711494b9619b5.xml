        <script><![CDATA[var ActionPlanPersistence = Class.create();
ActionPlanPersistence.prototype = {
    initialize: function() {
    },
	
	getSLATask: function(task, sla) {
		var grTask = new GlideRecord("task_sla");
		grTask.addQuery("task", task);
		grTask.addQuery("sla", sla);
		grTask.addQuery("end_time", "");
		grTask.query();
		
		return grTask;
	},
	
    type: 'ActionPlanPersistence'
};]]></script>
