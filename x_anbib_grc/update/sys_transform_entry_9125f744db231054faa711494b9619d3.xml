        <source_script><![CDATA[answer = (function transformEntry(source) {
	var serviceId = source.u_service_id.toString().toString();
	var grAppl = new GlideRecord('cmdb_ci_appl');
	grAppl.addQuery('correlation_id', serviceId);
	grAppl.query();
	while(grAppl.next()) {
		var correlationId = grAppl.correlation_id.toString();
		if(correlationId === serviceId) return grAppl.getUniqueValue().toString();
	}
	return "";
})(source);]]></source_script>
